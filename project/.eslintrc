{
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["warn", "single"],
        "curly": ["warn", "all"],
        "eqeqeq": "warn"

    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true
    }
}